{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src/@types"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "preserve",
    "lib": ["es6", "dom"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "allowJs": true,
    "types": []
  },
  "exclude": ["node_modules", "public", ".cache", "npm-packages-offline-cache"]
}
